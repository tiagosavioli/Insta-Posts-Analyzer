{
  "name": "content-finder-api",
  "version": "1.0.0",
  "description": "Sistema de Detecção de Bots no Instagram com Interface Web",
  "main": "dist/src/index.js",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node dist/src/index.js",
    "dev": "ts-node src/index.ts",
    "ranker": "npm run build && node dist/scripts/ranker.js",
    "server": "npm run build && node dist/src/index.js",
